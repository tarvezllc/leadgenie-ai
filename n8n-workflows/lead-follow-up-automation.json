              "value": "={{ $json.id }}"
            },
            {
              "name": "user_id",
              "value": "={{ $json.user_id }}"
            },
            {
              "name": "scheduled_at",
              "value": "={{ new Date(Date.now() + 24 * 60 * 60 * 1000).toISOString() }}"
            },
            {
              "name": "follow_up_type",
              "value": "call"
            },
            {
              "name": "notes",
              "value": "Automated follow-up for new lead"
            }
          ]
        },
        "options": {}
